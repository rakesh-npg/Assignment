{"ast":null,"code":"// import { api } from '@/service/api';\nimport Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nexport default {\n  name: \"HotelTable\",\n\n  data() {\n    return {\n      tableData: [],\n      flagData: {\n        \"editForm\": false\n      },\n      tableHeaders: [\"id\", \"hotel_name\", \"hotel_address\", \"customer_id\", \"delete\"],\n      hotelFormData: {\n        \"hotelName\": \"\",\n        \"hotelAddress\": \"\",\n        \"hotelOwner\": \"\"\n      }\n    };\n  },\n\n  mounted() {\n    this.read();\n  },\n\n  methods: {\n    async read() {\n      let cal = await axios.get('http://127.0.0.1:3333/hotel/read');\n      this.tableData = cal.data;\n      console.log(this.tableData);\n    },\n\n    async editData(data, colName) {\n      this.flagData.editForm = true;\n      console.log(data);\n      console.log(colName);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA;AACA;AACA;AACA;AACAA;AAEA;EACAC,kBADA;;EAEAC;IACA;MACAC,aADA;MAEAC;QACA;MADA,CAFA;MAKAC,4EALA;MAMAC;QACA,eADA;QAEA,kBAFA;QAGA;MAHA;IANA;EAaA,CAhBA;;EAiBAC;IACA;EACA,CAnBA;;EAoBAC;IACA;MACA;MACA;MACAC;IACA,CALA;;IAOA;MACA;MACAA;MACAA;IAEA;;EAZA;AApBA","names":["Vue","name","data","tableData","flagData","tableHeaders","hotelFormData","mounted","methods","console"],"sourceRoot":"src/components","sources":["HotelTable.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>Table</h1>\n        <v-simple-table>\n            <thead>\n               <tr>\n                    <th v-for=\"(colName) in tableHeaders\" :key=\"colName\">\n                      {{colName}}\n                    </th>\n               </tr>\n            </thead>\n            <tbody>\n                <tr \n                v-for=\"rowData in tableData\" :key=\"rowData.id\">\n                    <td v-for=\"(colName) in tableHeaders\" :key=\"colName\">\n                        {{rowData[colName]}}\n                        <template v-if=\"colName != 'id'\">\n                            <v-icon small @click=\"editData(rowData, colName)\">mdi-pencil</v-icon>\n                        </template>\n                        <template v-if=\"colName = 'delete'\">\n                            <v-icon small @click=\"editData(rowData, colName)\">mdi-delete</v-icon>\n                        </template>\n                    </td>\n                </tr>\n            </tbody>\n        </v-simple-table>\n    </div>\n</template>\n\n<script>\n// import { api } from '@/service/api';\nimport Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios)\n\nexport default{\n    name: \"HotelTable\", \n    data() {\n        return {\n            tableData: [], \n            flagData: {\n                \"editForm\":false \n            },\n            tableHeaders: [\"id\", \"hotel_name\", \"hotel_address\", \"customer_id\", \"delete\"], \n            hotelFormData: {\n                \"hotelName\": \"\", \n                \"hotelAddress\": \"\", \n                \"hotelOwner\":\"\", \n            },\n            \n        }\n    }, \n    mounted() {\n        this.read() \n    }, \n    methods:{\n        async read() {\n            let cal = await axios.get('http://127.0.0.1:3333/hotel/read')\n            this.tableData  = cal.data\n            console.log(this.tableData) \n        },\n\n        async editData(data, colName) { \n            this.flagData.editForm = true \n            console.log(data) \n            console.log(colName)\n            \n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}