head>\n            <tbody>\n                <tr \n                v-for=\"rowData in tableData\" :key=\"rowData.id\">\n                    <td v-for=\"(colName) in tableHeaders\" :key=\"colName\">\n                        {{rowData[colName]}}\n                        <template v-if=\"colName != 'id'\">\n                            <v-icon small @click=\"editData(rowData, colName)\">mdi-pencil</v-icon>\n                            <template v-if=\"colName == 'delete'\">\n                                <v-icon small @click=\"delteData(rowData, colName)\">mdi-delete</v-icon>\n                            </template>\n                        </template>\n                    </td>\n                </tr>\n            </tbody>\n        </v-simple-table>\n\n        <v-dialog\n        v-model=\"flagData.newEntryForm\">\n        <v-card>\n                <v-card-title class=\"text-h5 grey lighten-2\">\n                New Entry\n                </v-card-title>\n\n                <v-form\n                ref=\"form\"\n                lazy-validation\n                >\n                    <v-text-field\n                    v-model=\"hotelFormData.hotelName\"\n                    label=\"name\">\n                    Hotel Name\n                    </v-text-field>\n            \n                    <v-text-field\n                    v-model=\"hotelFormData.hotelDoorno\"\n                    label=\"phone\">\n                    DoorNo \n                    </v-text-field>\n        \n\n                    <v-text-field\n                    v-model=\"hotelFormData.hotelSt\"\n                    label=\"phone\">\n                    Street Name\n                    </v-text-field>\n        \n                    <v-text-field\n                    v-model=\"hotelFormData.hotelLandmark\"\n                    label=\"phone\">\n                    Landmark \n                    </v-text-field>\n        \n                    <v-text-field\n                    v-model=\"hotelFormData.hotelPincode\"\n                    label=\"phone\">\n                    Pincode \n                    </v-text-field>\n\n                    <v-text-field\n                    v-model=\"hotelFormData.hotelCustomer\"\n                    label=\"phone\">\n                    Customer ID\n                    </v-text-field>\n        \n        \n\n\n                    <v-btn @click=\"validateForm\">\n                    Submit\n                    </v-btn>\n                    \n                \n                </v-form>\n\n            </v-card>\n        </v-dialog>\n    </div>\n</template>\n\n<script>\n// import { api } from '@/service/api';\nimport Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios)\n\nexport default{\n    name: \"HotelTable\", \n    data() {\n        return {\n            tableData: [], \n            flagData: {\n                \"editForm\":false, \n                \"newEntryForm\":false, \n            },\n            tableHeaders: [\"id\", \"hotel_name\", \"hotel_address\", \"customer_id\", \"delete\"], \n            hotelFormData: {\n                \"hotelName\": \"\", \n                \"hotelAddress\": \"\", \n                \"hotelCustomer\":\"\", \n                \"hotelDoorno\": \"\", \n                \"hotelSt\":\"\", \n                \"hotelLandmark\": \"\", \n                \"hotelPincode\":\"\", \n            },\n            \n        }\n    }, \n    mounted() {\n        this.read() \n    }, \n    methods:{\n        async read() {\n            let cal = await axios.get('http://127.0.0.1:3333/hotel/read')\n            this.tableData  = cal.data\n            console.log(this.tableData) \n        },\n\n        async editData(data, colName) { \n            this.flagData.editForm = true \n            console.log(data) \n            console.log(colName)\n            \n        },\n\n        async deleteData(data) {\n            //delete \n        }\n    }\n}\n</script>"]},"metadata":{},